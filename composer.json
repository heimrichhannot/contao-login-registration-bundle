{
  "name": "heimrichhannot/contao-login-registration-bundle",
  "type": "contao-bundle",
  "description": "A frontend module that allows login and registration in one module.",
  "license": "GPL-3.0-or-later",
  "require": {
    "php": "^8.1",
    "contao/core-bundle": "^4.13 || ^5.0",
    "symfony/event-dispatcher-contracts": "^1.0 || ^2.0 || ^3.0",
    "symfony/http-foundation": "^5.4 || ^6.0",
    "symfony/http-kernel": "^5.4 || ^6.0",
    "symfony/security-core": "^5.4 || ^6.0"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\LoginRegistrationBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\LoginRegistrationBundle\\ContaoManager\\Plugin"
  },
  "require-dev": {
    "rector/rector": "^1.2",
    "contao/contao-rector": "dev-main",
    "symplify/easy-coding-standard": "^12.5",
    "phpstan/phpstan": "^1.12",
    "phpstan/phpstan-symfony": "^1.4",
    "contao/manager-plugin": "^2.13",
    "contao/core-bundle": "^4.13"
  }
}
